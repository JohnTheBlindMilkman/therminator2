[Model_parameters]
#  freeze-out Cart. time  [fm]
# default:  
T0 = 10.0

# radial size [fm]
# default:  
R = 17.85
 
# radial velocity [c]
# default:  
H  = 0.00731299
A  = 0.0


# Freeze-Out Temperature [MeV]
# default: 165.6
#Temperature = 41.
Temperature = 49.7

# chemistry type 
# defaults: chemical_potential, gamma_lambda
Chemistry = chemical_potential

# Lambda and gamma parameters [1]
# default:

GammaQ =  1.0
GammaS =  0.163
GammaC =  1.0


# Chemical potentials for Barion, Isospin (I_3), Strangeness and Charm [MeV]
# default:  
MuB = 774.69
MuI = -14.26
MuS =  123.44
MuC =  0.0
[Subdirectory]
# subdirectory to store events of this model
# default: sr/
EventSubDir = events_sr_Hubble_PML_plusError
